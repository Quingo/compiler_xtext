import config.json.*
import operations.*


operation Randomized_benchmarking(): bool[] {
    bool[2] output;
    using(q : qubit[2]) {
        H(q[0]);
        CZ(q[0],q[1]);
        S(q[0]);
        CZ(q[0],q[1]);
        S(q[0]);
        Z(q[0]);
        H(q[0]);
        output[0]=measure(q[0]);
        output[1]=measure(q[1]);
    }
    return output;
}
